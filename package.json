{
  "name": "hashlabs.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:hashlabs/hashlabs.github.io.git",
  "author": "Victor Tortolero <victormtortolero@gmail.com>",
  "license": "MIT",
  "deploy": "yarn build && firebase deploy",
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "bootstrap": "4.0.0-alpha.6",
    "command-line-args": "^4.0.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.2",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob-all": "^3.1.0",
    "img-loader": "^2.0.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.2",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "uglify-js": "^3.0.10",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1"
  },
  "scripts": {
    "copy-assets": "rm -rf assets && rsync _assets/ assets/ -av",
    "copy-service-worker": "rm _site/service-worker.js && cp service-worker.js _site/service-worker.js",
    "watch": "yarn copy-assets && webpack --watch",
    "serve": "JEKYLL_ENV=development jekyll serve --config _config.yml,_config.development.yml",
    "webpack-build": "yarn copy-assets && webpack",
    "jekyll-build-staging": "JEKYLL_ENV=staging jekyll build --config _config.yml,_config.staging.yml",
    "jekyll-build-production": "JEKYLL_ENV=production jekyll build --config _config.yml,_config.production.yml",
    "build-staging": "yarn webpack-build && yarn jekyll-build-staging && webpack --env.prod && yarn copy-service-worker",
    "build-production": "yarn webpack-build && yarn jekyll-build-production && webpack --env.prod && yarn copy-service-worker",
    "deploy": "yarn build-staging && firebase deploy"
  },
  "devDependencies": {
    "ignore-loader": "^0.1.2",
    "sw-precache-webpack-plugin": "^0.11.4"
  }
}
